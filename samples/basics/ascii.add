resetColor();

for (var i = 0; i < 16; i++)
{
	setForeground('Yellow');
	print('{0:X} ', i);
	resetColor();
	
	if (i not in {0, 1, 8, 9})
    {
        for (var j = 0; j < 16; j++)
        {
        	if (i == 7 && j == 15) continue;
            print('{0} ', chr(16 * i + j));
        }
    }
	
	println();
}

